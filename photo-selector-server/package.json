{
  "name": "photo-selector-server",
  "version": "1.0.0",
  "description": "User photo management service",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "test": "jest --detectOpenHandles --forceExit"
  },
  "author": "N D K Ariyasena <ndkariyasenagmail.com>",
  "license": "ISC",
  "_moduleAliases": {
    "~root": ".",
    "@src": "src",
    "@v1": "src/v1",
    "@ComConfig": "src/config",
    "@ComHelpers": "src/helpers",
    "@ComModules": "src/modules",
    "@v1Config": "src/v1/config",
    "@v1Modules": "src/v1/module"
  },
  "jest": {
